package com.ict.edu;

public class Ex02 {
 public static void main(String[] args) {
	// if ~ else ~ : 조건식이 참일때와 거짓일때 각각 나눠서 처리한다.
	// 형식 : if(조건식){
	// 조건식이 참일때 실행 문장 ; {
	// 조건식이 참일때 실행 문장 ;
    // } else {
	// 조건식이 거짓일때 실행 문장 ;
	// 조건식이 거짓일때 실행 문장 ;
	// }
	 
	// int k2 이 60 이상이면 합격, 60미만은 불합격
	 int k2 = 60;
	 String str = "합격" : "불합격" ;
	
	if (k2 > 60) {
		str = "합격" ;
		}
	 
	if (k2 < 60) {
		str = "불합격" ;
	}
	 
	System.out.println("결과 :" + str);
	
	// 홀수 짝수 판별
	 
	 
	// 대문자, 소문자 판별
	 
	 
	// 1 또는 3이면 남자, 아니면 여자 

	// 근무시간이 8시간까지는 시간당 8590이고 
	// 8시간을 초과한 시간 만큼은 1.5배 지급한다. 
	// 현재 근무한 시간이 10이다. 
	// 얼마를 받아야 하는가?
	 
	// 두 수 중 큰 값을 출력하시오.
 
 }
}
