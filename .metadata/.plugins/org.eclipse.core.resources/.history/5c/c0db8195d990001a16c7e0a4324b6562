package com.ict.edu;

public class Ex06 {
	public static void main(String[] args) {
		// 다중 for문 : for 문안에 다른 for문이 존재하는 것
		
		for (int i = 1; i < 4; i++) {
			for (int j = 1; j < 6; j++) {
				System.out.println("i="+ i + ", j="+j);
				
			}
		}
		
		int sum = 1 ;
		
		for (int i = 2; i < 9; i++) {
			sum = sum * i ;
			for (int j = 2; j < 9; j++) {
				sum = sum * j ;
				 System.out.println(i+j);
			}
		
		}
		
	}
}
